import javax.persistence.Column;
import javax.persistence.Table;

@Table(name = "SUPPLIER")
entity supplier {
	id {
		@Column(name = "SUPPID")
		suppid: EBigInteger
	}
	@Column(name = "NAME")
	name: string
	@Column(name = "STATUS")
	status: string
	@Column(name = "ADDR1")
	addr1: string
	@Column(name = "ADDR2")
	addr2: string
	@Column(name = "CITY")
	city: string
	@Column(name = "STATE")
	state: string
	@Column(name = "ZIP")
	zip: string
	@Column(name = "PHONE")
	phone: string
}