import com.manydesigns.portofino.database.model.annotations.Column;
import com.manydesigns.portofino.database.model.annotations.Table;

@Table(name = "LINEITEM")
entity lineitem {
	id {
		@Column(name = "ORDERID")
		orderid: EBigInteger
		@Column(name = "LINENUM")
		linenum: EBigInteger
	}
	@Column(name = "ITEMID")
	itemid: string
	@Column(name = "QUANTITY")
	quantity: EBigInteger
	@Column(name = "UNITPRICE")
	unitprice: EBigDecimal
}