import com.manydesigns.portofino.database.model.annotations.Column;
import com.manydesigns.portofino.database.model.annotations.Table;

@Table(name = "ACCOUNT")
entity account {
	id {
		@Column(name = "USERID", jdbcType = 12, columnType = "VARCHAR", length = 80)
		userid
	}
	@Column(name = "EMAIL")
	email: string
	@Column(name = "FIRSTNAME")
	firstname: string
	@Column(name = "LASTNAME")
	lastname: string
	@Column(name = "STATUS")
	status: string
	@Column(name = "ADDR1")
	addr1: string
	@Column(name = "ADDR2")
	addr2: string
	@Column(name = "CITY")
	city: string
	@Column(name = "STATE")
	state: string
	@Column(name = "ZIP")
	zip: string
	@Column(name = "COUNTRY")
	country: string
	@Column(name = "PHONE")
	phone: string
}