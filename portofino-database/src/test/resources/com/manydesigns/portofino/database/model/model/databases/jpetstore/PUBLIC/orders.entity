import java.sql.Date;
import com.manydesigns.portofino.database.model.annotations.Column;
import com.manydesigns.portofino.database.model.annotations.Table;

@Table(name = "ORDERS")
entity orders {
	id {
		@Column(name = "ORDERID")
		orderid: EBigInteger
	}
	@Column(name = "USERID")
	userid: string
	@Column(name = "ORDERDATE")
	orderdate: Date
	@Column(name = "SHIPADDR1")
	shipaddr1: string
	@Column(name = "SHIPADDR2")
	shipaddr2: string
	@Column(name = "SHIPCITY")
	shipcity: string
	@Column(name = "SHIPSTATE")
	shipstate: string
	@Column(name = "SHIPZIP")
	shipzip: string
	@Column(name = "SHIPCOUNTRY")
	shipcountry: string
	@Column(name = "BILLADDR1")
	billaddr1: string
	@Column(name = "BILLADDR2")
	billaddr2: string
	@Column(name = "BILLCITY")
	billcity: string
	@Column(name = "BILLSTATE")
	billstate: string
	@Column(name = "BILLZIP")
	billzip: string
	@Column(name = "BILLCOUNTRY")
	billcountry: string
	@Column(name = "COURIER")
	courier: string
	@Column(name = "TOTALPRICE")
	totalprice: EBigDecimal
	@Column(name = "BILLTOFIRSTNAME")
	billtofirstname: string
	@Column(name = "BILLTOLASTNAME")
	billtolastname: string
	@Column(name = "SHIPTOFIRSTNAME")
	shiptofirstname: string
	@Column(name = "SHIPTOLASTNAME")
	shiptolastname: string
	@Column(name = "CREDITCARD")
	creditcard: string
	@Column(name = "EXPRDATE")
	exprdate: string
	@Column(name = "CARDTYPE")
	cardtype: string
	@Column(name = "LOCALE")
	locale: string
}