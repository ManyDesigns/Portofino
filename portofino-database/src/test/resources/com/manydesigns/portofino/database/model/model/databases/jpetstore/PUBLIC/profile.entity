import com.manydesigns.portofino.database.model.annotations.Column;
import com.manydesigns.portofino.database.model.annotations.Table;

@Table(name = "PROFILE")
entity profile {
	id {
		@Column(name = "USERID")
		userid: string
	}
	@Column(name = "LANGPREF")
	langpref: string
	@Column(name = "FAVCATEGORY")
	favcategory: string
	@Column(name = "MYLISTOPT")
	mylistopt: boolean
	@Column(name = "BANNEROPT")
	banneropt: boolean
}