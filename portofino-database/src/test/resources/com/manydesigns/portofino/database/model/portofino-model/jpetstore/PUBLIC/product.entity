import javax.persistence.Column;
import javax.persistence.Table;

@Table(name = "PRODUCT")
entity product {
	id {
		@Column(name = "PRODUCTID")
		productid: string
	}
	@Column(name = "CATEGORY")
	category: string
	@Column(name = "NAME")
	name: string
	@Column(name = "DESCN")
	descn: string
	FK_PRODUCT_1 --> category(catid=category)
}