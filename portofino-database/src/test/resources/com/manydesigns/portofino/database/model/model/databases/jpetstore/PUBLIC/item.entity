import com.manydesigns.portofino.database.model.annotations.Column;
import com.manydesigns.portofino.database.model.annotations.Table;

@Table(name = "ITEM")
entity item {
	id {
		@Column(name = "ITEMID")
		itemid: string
	}
	@Column(name = "PRODUCTID")
	productid: string
	@Column(name = "LISTPRICE")
	listprice: EBigDecimal
	@Column(name = "UNITCOST")
	unitcost: EBigDecimal
	@Column(name = "SUPPLIER")
	supplier: EBigInteger
	@Column(name = "STATUS")
	status: string
	@Column(name = "ATTR1")
	attr1: string
	@Column(name = "ATTR2")
	attr2: string
	@Column(name = "ATTR3")
	attr3: string
	@Column(name = "ATTR4")
	attr4: string
	@Column(name = "ATTR5")
	attr5: string
	FK_ITEM_2 --> supplier(suppid=supplier)
	FK_ITEM_1 --> product(productid=productid)
}